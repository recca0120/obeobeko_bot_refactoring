{
  "globals": {
      "google": "true"
  },
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "no-restricted-syntax": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-return-assign": 0,
    "no-param-reassign": 0,
    "global-require": 0,
    "indent": ["error", 4]
  }
}
